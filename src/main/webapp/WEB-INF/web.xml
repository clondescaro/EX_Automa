<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
    <display-name>ctaCorriente</display-name>

   <servlet>
    <servlet-name>NombreDelServlet</servlet-name>
    <servlet-class>paquete.y.clase.completa.NombreDeClaseServlet</servlet-class>
</servlet>
<servlet-mapping>
    <servlet-name>NombreDelServlet</servlet-name>
    <url-pattern>/PatronDeURL</url-pattern>
</servlet-mapping>

    <resource-ref>
    	<description>DataSource for ctaCorriente Application</description>
    		<res-ref-name>jdbc/ctaCorriente</res-ref-name>
   	 		<res-type>javax.sql.DataSource</res-type>
    	<res-auth>Container</res-auth>
	</resource-ref>
    
</web-app>
